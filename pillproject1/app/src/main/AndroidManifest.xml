<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lucyeberhard.pillproject1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/dosediary_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/dosediary_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ShowLogActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".AboutActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".NewTrackerActivity"
            android:label="@string/title_activity_new_tracker"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.lucyeberhard.pillproject1.MainActivity" />
        </activity>
        <activity
            android:name=".CountdownActivity"
            android:label="@string/title_activity_countdown"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>